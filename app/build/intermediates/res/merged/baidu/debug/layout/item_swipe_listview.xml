<?xml version="1.0" encoding="utf-8"?>

<com.touyuanren.hahahuyu.widget.swipelistview.SwipeHorizontalMenuLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:sml="http://schemas.android.com/apk/res-auto"
    android:id="@+id/sml"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    sml:sml_auto_open_percent="0.2"
    sml:sml_scroller_duration="250"
    sml:sml_scroller_interpolator="@android:anim/bounce_interpolator">
    <include
        android:id="@id/smContentView"
        layout="@layout/mb_list_activity" />

    <include
        android:id="@id/smMenuViewRight"
        layout="@layout/item_simple_right_menu" />


</com.touyuanren.hahahuyu.widget.swipelistview.SwipeHorizontalMenuLayout>
